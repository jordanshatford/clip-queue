{
  "name": "clip-queue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest run --environment jsdom",
    "test:coverage": "vitest run --environment jsdom --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@sentry/tracing": "^6.19.3",
    "@sentry/vue": "^6.19.3",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "phosphor-vue": "^4.4.0",
    "pinia": "^2.0.12",
    "tmi.js": "npm:@coldino/tmi.js-clientonly-fork@^1.8.1-clientonly",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.9",
    "@rushstack/eslint-patch": "^1.1.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@types/tmi.js": "^1.8.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.5.9",
    "vue-tsc": "^0.38.5"
  }
}
