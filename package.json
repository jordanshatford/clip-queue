{
  "name": "clip-queue",
  "version": "1.13.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "web": "pnpm --filter @cq/web -- ",
    "cfg": "pnpm --filter @cq/config -- ",
    "player": "pnpm --filter @cq/player -- ",
    "providers": "pnpm --filter @cq/providers -- ",
    "services": "pnpm --filter @cq/services -- ",
    "sources": "pnpm --filter @cq/sources -- ",
    "ui": "pnpm --filter @cq/ui -- ",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "lint:fix": "pnpm --recursive lint:fix",
    "format": "pnpm --recursive format",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "pnpm --recursive typecheck",
    "deps:update": "corepack use pnpm@latest && pnpm up --latest && pnpm --recursive up --latest",
    "commitlint": "commitlint --edit",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "typescript": "catalog:",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f",
  "engines": {
    "pnpm": ">=10",
    "node": ">=20"
  }
}
