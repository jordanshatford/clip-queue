{
  "name": "clip-queue",
  "version": "1.13.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "web": "pnpm --filter @cq/web -- ",
    "cfg": "pnpm --filter @cq/config -- ",
    "player": "pnpm --filter @cq/player -- ",
    "providers": "pnpm --filter @cq/providers -- ",
    "services": "pnpm --filter @cq/services -- ",
    "ui": "pnpm --filter @cq/ui -- ",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "lint:fix": "pnpm --recursive lint:fix",
    "format": "pnpm --recursive format",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "pnpm --recursive typecheck",
    "commitlint": "commitlint --edit",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@vitest/coverage-v8": "^1.5.0",
    "husky": "^9.0.11",
    "vitest": "^1.5.0"
  },
  "packageManager": "pnpm@8.15.7+sha256.50783dd0fa303852de2dd1557cd4b9f07cb5b018154a6e76d0f40635d6cee019",
  "engines": {
    "pnpm": ">=8",
    "node": ">=18"
  }
}
