{
  "name": "@cq/web",
  "version": "1.31.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && pnpm typecheck && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --build --force",
    "lint": "prettier --check . && eslint . && pnpx @inlang/cli lint --project ./project.inlang",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
  },
  "dependencies": {
    "@cq/player": "workspace:*",
    "@cq/providers": "workspace:*",
    "@cq/services": "workspace:*",
    "@cq/sources": "workspace:*",
    "@cq/ui": "workspace:*",
    "@sentry/vue": "^8.51.0",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cq/config": "workspace:*",
    "@inlang/paraglide-js": "1.11.8",
    "@inlang/paraglide-vite": "^1.3.5",
    "@pinia/testing": "^0.1.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0"
  }
}
