{
  "name": "@cq/web",
  "version": "1.21.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && pnpm typecheck && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --build --force",
    "lint": "prettier --check . && eslint . && pnpx @inlang/cli lint --project ./project.inlang",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
  },
  "dependencies": {
    "@cq/player": "workspace:*",
    "@cq/providers": "workspace:*",
    "@cq/services": "workspace:*",
    "@cq/ui": "workspace:*",
    "@sentry/vue": "^8.40.0",
    "pinia": "^2.2.6",
    "pinia-plugin-persistedstate": "^4.1.3",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@cq/config": "workspace:*",
    "@inlang/paraglide-js": "1.11.3",
    "@inlang/paraglide-vite": "^1.2.77",
    "@pinia/testing": "^0.1.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  }
}
